.mb-4 = form_for [:admin, @file_upload] do |f|
  = render 'admin/shared/error_messages', resource: @file_upload

  .form-group
    .field
      = f.label :description
      = f.text_field :description, class: 'form-control'
  .form-group
    .field
      = f.file_field :file
  / i18n-tasks-use t('helpers.submit.file_upload.create')
  .actions = f.submit class: 'btn btn-primary'
